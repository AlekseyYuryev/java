<component-set xmlns="http://plexus.codehaus.org/xsd/components">
  <components>
    <component>
      <role>org.apache.maven.lifecycle.Lifecycle</role>
      <role-hint>xdb</role-hint>
      <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
      <configuration>
        <id>xdb</id>
        <default-phases>
          <generate-sources>org.safris.maven.plugin:xdb-maven-plugin:entities</generate-sources>
          <generate-resources>org.safris.maven.plugin:xdb-maven-plugin:schema</generate-resources>
        </default-phases>
      </configuration>
    </component>
    <component>
      <role>org.apache.maven.lifecycle.Lifecycle</role>
      <role-hint>xdb</role-hint>
      <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
      <configuration>
        <id>xdb</id>
        <phases>
          <phase>entities</phase>
          <phase>schema</phase>
        </phases>
        <default-phases>
          <entities>org.safris.maven.plugin:xdb-maven-plugin:entities</entities>
          <schema>org.safris.maven.plugin:xdb-maven-plugin:schema</schema>
        </default-phases>
      </configuration>
    </component>
  </components>
</component-set>