        <dependency>
          <groupId>org.safris.commons</groupId>
          <artifactId>commons-logging</artifactId>
        </dependency>
        <dependency>
          <groupId>org.safris.commons</groupId>
          <artifactId>commons-test</artifactId>
          <version>1.1.6</version>
          <scope>test</scope>
        </dependency>
      </dependencies>
    </profile>
    <profile>
      <id>core</id>
      <activation>
        <property>
          <name>core</name>
        </property>
      </activation>
      <modules>
        <module>commons</module>
        <module>maven</module>
      </modules>
    </profile>
